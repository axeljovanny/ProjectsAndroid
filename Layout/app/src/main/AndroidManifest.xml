<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.edu.utng.layout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".FrameLayoutActivity">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN"></action>
                <category
                    android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".LinearLayoutActivity"></activity>
        <activity android:name=".TableLayoutActivity"></activity>
        <activity android:name=".RelativeLayoutActivity"></activity>
        <activity android:name=".EntradaActivity"></activity>
    </application>

</manifest>
