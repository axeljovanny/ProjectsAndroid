<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="6dp" />
            <gradient android:startColor="@color/community_dark_button_center" android:endColor="@color/community_dark_button_start" android:angle="90.0" android:type="linear" android:centerColor="@color/community_dark_button_center" />
            <stroke android:width="2px" android:color="@color/community_dark_button_stroke" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="6dp" />
            <gradient android:startColor="@color/community_dark_button_start" android:endColor="@color/community_dark_button_end" android:angle="90.0" android:type="linear" android:centerColor="@color/community_dark_button_center" />
            <stroke android:width="2px" android:color="@color/community_dark_button_stroke" />
        </shape>
    </item>
</selector>
